<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :client %>
    <%= f.input :hobbies %>
    <%= f.input :text %>
    <%= f.input :profile_pic %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
